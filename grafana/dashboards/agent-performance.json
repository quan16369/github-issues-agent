{
  "dashboard": {
    "title": "GitHub Issue Agent - Agent Performance",
    "uid": "github-agent-agents",
    "tags": ["github-agent", "agents", "performance"],
    "timezone": "browser",
    "schemaVersion": 36,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "title": "Agent Execution Rate (executions/sec)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent) (rate(agent_executions_total{status=\"success\"}[5m]))",
            "legendFormat": "{{agent}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "exec/sec", "format": "short"},
          {"show": false}
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "max": true,
          "values": true
        }
      },
      {
        "title": "Agent Execution Duration (seconds)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "rate(agent_execution_duration_seconds_sum{agent=\"input_guardrail\"}[5m]) / rate(agent_execution_duration_seconds_count{agent=\"input_guardrail\"}[5m])",
            "legendFormat": "Input Guardrail (avg)",
            "refId": "A"
          },
          {
            "expr": "rate(agent_execution_duration_seconds_sum{agent=\"issue_search\"}[5m]) / rate(agent_execution_duration_seconds_count{agent=\"issue_search\"}[5m])",
            "legendFormat": "Issue Search (avg)",
            "refId": "B"
          },
          {
            "expr": "rate(agent_execution_duration_seconds_sum{agent=\"classification\"}[5m]) / rate(agent_execution_duration_seconds_count{agent=\"classification\"}[5m])",
            "legendFormat": "Classification (avg)",
            "refId": "C"
          },
          {
            "expr": "rate(agent_execution_duration_seconds_sum{agent=\"recommendation\"}[5m]) / rate(agent_execution_duration_seconds_count{agent=\"recommendation\"}[5m])",
            "legendFormat": "Recommendation (avg)",
            "refId": "D"
          },
          {
            "expr": "rate(agent_execution_duration_seconds_sum{agent=\"output_guardrail\"}[5m]) / rate(agent_execution_duration_seconds_count{agent=\"output_guardrail\"}[5m])",
            "legendFormat": "Output Guardrail (avg)",
            "refId": "E"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"show": false}
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "max": true,
          "values": true
        }
      },
      {
        "title": "Input Guardrail Stats",
        "type": "stat",
        "gridPos": {"x": 0, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(agent_executions_total{agent=\"input_guardrail\",status=\"success\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "title": "Issue Search Stats",
        "type": "stat",
        "gridPos": {"x": 6, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(agent_executions_total{agent=\"issue_search\",status=\"success\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "title": "Classification Stats",
        "type": "stat",
        "gridPos": {"x": 12, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(agent_executions_total{agent=\"classification\",status=\"success\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "title": "Recommendation Stats",
        "type": "stat",
        "gridPos": {"x": 18, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(agent_executions_total{agent=\"recommendation\",status=\"success\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "title": "Agent Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent) (rate(agent_executions_total{status=\"error\"}[5m]))",
            "legendFormat": "{{agent}} errors",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "errors/sec", "format": "short"},
          {"show": false}
        ]
      },
      {
        "title": "Agent P95 Latency (seconds)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (agent, le) (rate(agent_execution_duration_seconds_bucket[5m])))",
            "legendFormat": "{{agent}} (P95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"show": false}
        ]
      },
      {
        "title": "Guardrail Blocks by Agent",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent) (rate(guardrail_blocks_total[5m]))",
            "legendFormat": "{{agent}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "blocks/sec", "format": "short"},
          {"show": false}
        ]
      }
    ]
  }
}
